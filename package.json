{
  "name": "evApp",
  "version": "1.0.0",
  "scripts": {
    "dev": "blitz dev",
    "build": "yarn blitz prisma migrate deploy && yarn blitz prisma generate && yarn email-install-dependencies && yarn blitz build",
    "email-install-dependencies": "cd mailers/react-email-starter && yarn install",
    "email": "cd mailers/react-email-starter && yarn dev --port 3009",
    "start": "blitz start",
    "db:studio": "BROWSER=none yarn blitz prisma studio",
    "db:migrate": "yarn blitz prisma migrate dev",
    "lint": "eslint --ignore-path .gitignore --ext .js,.ts,.tsx .",
    "test": "vitest run --passWithNoTests",
    "test:watch": "vitest",
    "prepare": "husky install"
  },
  "prisma": {
    "schema": "db/schema.prisma"
  },
  "prettier": {
    "semi": false,
    "printWidth": 100
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@blitzjs/auth": "2.0.5",
    "@blitzjs/next": "2.0.5",
    "@blitzjs/rpc": "2.0.5",
    "@emotion/react": "11.11.3",
    "@emotion/server": "11.11.0",
    "@hookform/error-message": "2.0.1",
    "@hookform/resolvers": "2.9.11",
    "@mantine/core": "7.5.3",
    "@mantine/ds": "7.2.2",
    "@mantine/form": "7.6.1",
    "@mantine/hooks": "7.5.3",
    "@mantine/modals": "7.7.1",
    "@mantine/next": "6.0.21",
    "@mantine/notifications": "7.6.1",
    "@mantine/prism": "6.0.21",
    "@prisma/client": "4.6.1",
    "@t3-oss/env-nextjs": "0.9.2",
    "@tabler/icons-react": "2.47.0",
    "@uploadthing/react": "6.4.1",
    "blitz": "2.0.5",
    "date-fns": "3.4.0",
    "lodash": "4.17.21",
    "next": "13.5.4",
    "prisma": "4.6.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hanger": "2.4.5",
    "react-hook-form": "7.50.1",
    "resend": "3.2.0",
    "secure-password": "4.0.0",
    "uploadthing": "6.7.0",
    "zod": "3.20.2",
    "zod-error": "1.5.0"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "12.0.8",
    "@next/env": "13.4.19",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.4.0",
    "@testing-library/react-hooks": "8.0.1",
    "@types/node": "18.11.9",
    "@types/preview-email": "2.0.1",
    "@types/react": "18.2.58",
    "@typescript-eslint/eslint-plugin": "5.42.1",
    "@vitejs/plugin-react": "2.2.0",
    "ajv": "^7.0.0",
    "eslint": "8.27.0",
    "eslint-config-next": "12.3.1",
    "eslint-config-prettier": "8.5.0",
    "husky": "8.0.2",
    "jsdom": "20.0.3",
    "lint-staged": "13.0.3",
    "prettier": "^2.7.1",
    "prettier-plugin-prisma": "4.4.0",
    "pretty-quick": "3.1.3",
    "preview-email": "3.0.7",
    "typescript": "4.9.4",
    "vite-tsconfig-paths": "3.6.0",
    "vitest": "0.25.3"
  },
  "private": true
}
